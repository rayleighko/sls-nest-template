{
  "name": "sls-nest-template",
  "version": "0.1.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "^16.17.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "cp:env:dev": "cp .env.development .env",
    "cp:env:prod": "cp .env.production .env",
    "prestart:dev": "yarn cp:env:dev",
    "start:dev": "nest start --watch",
    "prestart:debug": "yarn cp:env:dev",
    "start:debug": "nest start --debug --watch",
    "prestart:prod": "yarn cp:env:prod && yarn build",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "presls:deploy": "yarn cp:env:prod && yarn build",
    "sls:deploy": "sls deploy --aws-profile default --stage prod --verbose",
    "presls:offline": "yarn cp:env:dev && yarn build",
    "sls:offline": "sls offline start",
    "sls:package": "sls package"
  },
  "dependencies": {
    "@anchan828/typeorm-decorators": "^1.0.48",
    "@apollo/server": "^4.3.3",
    "@aws-sdk/client-s3": "^3.272.0",
    "@aws-sdk/s3-request-presigner": "^3.272.0",
    "@nestjs/apollo": "^10.2.0",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^10.2.0",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/terminus": "^9.2.0",
    "@nestjs/typeorm": "^9.0.1",
    "@ntegral/nestjs-sentry": "^4.0.0",
    "@sentry/hub": "^7.37.1",
    "@sentry/node": "^7.37.1",
    "@vendia/serverless-express": "^4.10.1",
    "aws-lambda": "^1.0.7",
    "axios": "^1.3.2",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "graphql": "^16.6.0",
    "helmet": "^6.0.1",
    "morgan": "^1.10.0",
    "nestjs-i18n": "^10.2.6",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.9.0",
    "reflect-metadata": "^0.1.13",
    "request-context": "^2.0.0",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "source-map-support": "^0.5.21",
    "typeorm": "^0.3.12",
    "typeorm-transactional-cls-hooked": "^0.1.21",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/aws-lambda": "^8.10.110",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/mime-types": "^2.1.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "18.11.18",
    "@types/passport-local": "^1.0.35",
    "@types/source-map-support": "^0.5.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.3.1",
    "prettier": "^2.8.4",
    "serverless": "^3.27.0",
    "serverless-dotenv-plugin": "^4.0.2",
    "serverless-jetpack": "^0.11.1",
    "serverless-offline": "^12.0.4",
    "supertest": "^6.1.3",
    "terser-webpack-plugin": "^5.3.6",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4",
    "webpack": "^5.75.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
